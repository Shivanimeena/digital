{
  "name": "ds",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-path=dist/browser",
    "postinstall": "npm run build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ds": "node dist/ds/server/server.mjs",
    "build:ssr": "ng build && ng run your-app-name:server",
    "serve:ssr": "node dist/server/main.js"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/forms": "^20.1.6",
    "@angular/platform-browser": "^20.1.6",
    "@angular/platform-server": "^20.1.6",
    "@angular/router": "^20.1.6",
    "@angular/ssr": "^20.1.5",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@netlify/angular-runtime": "^2.4.0",
    "aos": "^2.3.4",
    "express": "^5.1.0",
    "rxjs": "~7.8.2",
    "swiper": "^11.2.10",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.6",
    "@types/aos": "^3.0.7",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^24.2.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.8.3"
  }
}
